rm -rf *.bz2 *.db

# lynx gives cert errors
# for file in `lynx --dump https://dist.whosonfirst.org/sqlite/ | fgrep https:// | fgrep venue- | fgrep latest | awk '{ print $2; }'`; do
for file in `curl -s https://dist.whosonfirst.org/sqlite/ | fgrep venue- | fgrep latest.db.bz2 |  awk '{gsub("<[^>]*>", "")}1' | awk '{ print "https://dist.whosonfirst.org/sqlite/"$1".bz2"; }'`; do
	echo $file:
	wget --no-check-certificate $file
	wait
	bunzip2 `basename $file`&
done

wait
